	Journal = {Genome Biol},
	Journal-Full = {Genome biology},
	Mesh = {Algorithms; DNA, Complementary; Female; Gene Expression Profiling; Humans; RNA; Reference Standards; Reproducibility of Results; Reverse Transcriptase Polymerase Chain Reaction; Time Factors; Tumor Cells, Cultured},
	Month = {Jun},
	Number = {7},
	Pages = {RESEARCH0034},
	Pmc = {PMC126239},
	Pmid = {12184808},
	Pst = {ppublish},
	Title = {Accurate normalization of real-time quantitative RT-PCR data by geometric averaging of multiple internal control genes},
	Volume = {3},
	Year = {2002}}

