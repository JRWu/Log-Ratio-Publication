	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {amino acid coevolution; genetic selection},
	Mesh = {Aspartic Acid; Binding Sites; Biocatalysis; Catalytic Domain; Endonucleases; Evolution, Molecular; Glutamic Acid; Models, Genetic; Models, Molecular; Mutation; Phylogeny; Protein Structure, Tertiary},
