	Keywords = {16S rRNA gene sequencing; Compositional data; Microbiome; Multivariate analysis},
	Month = {May},
	Number = {5},
	Pages = {322-9},
	Pmid = {27143475},
	Pst = {ppublish},
	Title = {It's all relative: analyzing microbiome data as compositions},
	Volume = {26},
	Year = {2016},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.annepidem.2016.03.003}}

