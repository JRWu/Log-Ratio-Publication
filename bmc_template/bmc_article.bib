	Mesh = {Analysis of Variance; Bacillus cereus; Gardnerella vaginalis; Gene Expression; Gene Expression Profiling; High-Throughput Nucleotide Sequencing; Humans; Kidney; Lactobacillus; Liver; Megasphaera; Metagenome; Prevotella; RNA; RNA, Bacterial; Reproducibility of Results; Sample Size; Sequence Analysis, RNA},
	Number = {7},
	Pages = {e67019},
